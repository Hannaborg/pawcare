<h1>this is a dog board!!</h1>
<% current_user.dogs.each do |dog| %>
<% if dog.photo.attached? %>
  <%= cl_image_tag dog.photo.key, height: 100, width: 100, crop: :fill %>
<% else %>
  <img src="https://images.unsplash.com/photo-1534351450181-ea9f78427fe8?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=668&q=80"width="100" height="100">
<% end  %>
  <h1><%= dog.name %></h1>
  <p style="color: black">Age: <%= dog.age %></p>
  <p style="color: black">Age: <%= dog.breed %></p>
   <p><%= link_to "Delete", dog_path(dog), method: :delete %></p>
 <% end %>

 <div class="plain-text-button">
  <p><%= link_to "< Back", dogs_path %></p>
</div>
