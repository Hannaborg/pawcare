
<% @bookings.each do |booking| %>
  <% if booking.dog.photo.attached? %>
    <%= cl_image_tag booking.dog.photo.key, height: 100, width: 100, crop: :fill %>
  <% else %>
    <img src="https://images.unsplash.com/photo-1534351450181-ea9f78427fe8?ixid=MXwxMjA3fDB8MHxwaG90by1wYWdlfHx8fGVufDB8fHw%3D&ixlib=rb-1.2.1&auto=format&fit=crop&w=668&q=80"width="100" height="100">
  <% end  %>
      <h1><%= booking.dog.name %></h1>
      <p style="color: black">Start: <%= booking.start_date %> End: <%= booking.end_date %></p>
      <p><%= link_to "Cancel", booking_path(booking), method: :delete %></p>
 <% end %>


<div class="plain-text-button">
  <p><%= link_to "< Back", dogs_path %></p>
</div>

